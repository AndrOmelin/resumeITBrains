<div>
       {{#with main}}
    <main>
       <section>
            {{> title data="My Education" big=true}}
          <ul>
             {{#each educations}}
                <li>{{name}}
                   {{!-- {{#unless isEnd=true}} (Ще в процесі) {{/unless}} --}}
                   {{#unless isEnd}}(У процесі вивчення){{/unless}}
                </li>
             {{/each}}
          </ul>
       </section>

       <section>
            {{> title data="My Сertificates"}}
          <ul>
             {{#each certificates}}
                <li>
                   {{name}} ({{id}})
                </li>
             {{/each}}
          </ul>
       </section>
    </main>
  {{/with}}
</div>